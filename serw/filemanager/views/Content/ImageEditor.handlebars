<link rel="stylesheet" href="/scss/Content/FileEditor/FileEditor.css" />
<div class="container" id="image-editor">
  <div class="nav">
    <div class="nav-item">
      <button class="btn btn-link" id="save-file">
        <i class="fa-solid fa-floppy-disk"></i>
      </button>
    </div>
    <div class="nav-item">
      <button class="btn btn-link" id="cancel-file">
        <i class="fa-solid fa-xmark"></i>
      </button>
    </div>
  </div>
  <div id="editor">
    <div id="image"></div>
  </div>
  <div id="content">
    <canvas id="image-canvas" width="1000" height="800"></canvas>
  </div>
</div>
<script>
  var path = "{{context.path}}"; var file = "{{context.file}}"; var image = "{{context.data}}";
</script>
<script src="/js/build/ImageEditor.js" type="module">
</script>